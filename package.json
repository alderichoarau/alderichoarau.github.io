{
  "name": "alderichoarau-portfolio-temp",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:gh-pages": "ng build --configuration production --base-href=/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false --code-coverage=false --browsers=ChromeHeadless",
    "test:watch": "ng test",
    "test:coverage": "ng test --watch=false --code-coverage=true --browsers=ChromeHeadless",
    "lint": "ng lint --max-warnings=0",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write \"src/**/*.{ts,js,html,scss,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,html,scss,css,json}\"",
    "check": "npm run lint && npm run format:check",
    "pre-commit": "lint-staged",
    "pre-commit-full": "npm run test && npm run lint && npm run build:prod",
    "quality-gate": "npm run format:check && npm run lint && npm run test && npm run build:prod",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.2.15",
    "@angular/cdk": "20.2.9",
    "@angular/common": "19.2.15",
    "@angular/compiler": "19.2.15",
    "@angular/core": "19.2.15",
    "@angular/forms": "19.2.15",
    "@angular/material": "20.2.9",
    "@angular/platform-browser": "19.2.15",
    "@angular/platform-browser-dynamic": "19.2.15",
    "@angular/router": "19.2.15",
    "@angular/service-worker": "19.2.15",
    "@ngx-translate/core": "17.0.0",
    "@ngx-translate/http-loader": "17.0.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.17",
    "@angular/cli": "19.2.17",
    "@angular/compiler-cli": "19.2.15",
    "@types/jasmine": "5.1.12",
    "angular-eslint": "20.4.0",
    "eslint": "9.38.0",
    "husky": "9.1.7",
    "jasmine-core": "5.12.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "lint-staged": "16.2.4",
    "prettier": "3.6.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.46.1"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{html,scss,css,json,md}": [
      "prettier --write"
    ]
  }
}