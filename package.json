{
  "name": "alderichoarau-portfolio-temp",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:gh-pages": "ng build --configuration production --base-href=/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false --code-coverage=false --browsers=ChromeHeadless",
    "test:watch": "ng test",
    "test:coverage": "ng test --watch=false --code-coverage=true --browsers=ChromeHeadless",
    "lint": "ng lint --max-warnings=0",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write \"src/**/*.{ts,js,html,scss,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,html,scss,css,json}\"",
    "check": "npm run lint && npm run format:check",
    "pre-commit": "lint-staged",
    "pre-commit-full": "npm run test && npm run lint && npm run build:prod",
    "quality-gate": "npm run format:check && npm run lint && npm run test && npm run build:prod",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.1",
    "@angular/cdk": "^20.2.4",
    "@angular/common": "^20.3.1",
    "@angular/compiler": "^20.3.1",
    "@angular/core": "^20.3.1",
    "@angular/forms": "^20.3.1",
    "@angular/material": "^20.2.4",
    "@angular/platform-browser": "^20.3.1",
    "@angular/platform-browser-dynamic": "^20.3.1",
    "@angular/router": "^20.3.1",
    "@angular/service-worker": "^20.3.1",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.2",
    "@angular/cli": "^20.3.2",
    "@angular/compiler-cli": "^20.3.1",
    "@types/jasmine": "~5.1.9",
    "angular-eslint": "20.3.0",
    "eslint": "^9.35.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.10.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "typescript": "~5.5.4",
    "typescript-eslint": "8.40.0"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{html,scss,css,json,md}": [
      "prettier --write"
    ]
  }
}